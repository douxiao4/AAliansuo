.room-type {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  display: flex;
  background: #EEEEEE;
  color: #A2A7AA;
}
.room-type view {
  flex: 1;
  text-align: center;
  color: #455055;
}
.room-type .active {
  background: #fff;
  color: #040000;
  font-weight: bold;
  font-size: 28rpx;
}

.room-list-wrap {

}
.select-day {
  width: 100%;
  /*height: 100rpx;*/
  /*line-height: 100rpx;*/
  /*text-align: center;*/
  /* border-bottom: 1px solid #EBEDF0; */
}
.room-list {

}
.room-item {
  width: 100%;
  border-bottom: 1px solid #EBEDF0;
}
.room-type-name {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  font-size: 30rpx;
}
.room-type-name .title {
  flex: 5;
  text-align: left;
  margin-left: 50rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.room-type-name .price {
  flex: 5;
  color: #F05442; 
}
.room-type-name .price text {
  font-size: 26rpx;
  color: #798394;
}
.room-type-name .switch-more {
  flex: 1;
  margin-right: 50rpx;
  text-align: right;
  color: #C8C8CC;
}
.room-level-price {
  background-color: #F6F8FB;
}
.level-item {
  width: 100%;
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #E6E7E8;
}
.level-item text {
  font-size: 30rpx;
}
.level-item .level-name {
  flex: 5;
  color: #000;
  font-size: 25rpx;
  margin-left: 60rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.level-item .level-price {
  flex: 2;
  color: #F05442;
}
.level-item .level-btn {
  flex: 2;
  color: #fff;
  background-color: #F05442;
  border-radius: 4px;
  text-align: center;
  margin: 20rpx 50rpx;
  line-height: 60rpx;
  font-size: 26rpx;
}
.level-item .level-btn.disabled-btn {
  background-color: #aaa;
}
.show-more-btn {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 30rpx;
  color: #798394;
}
.comment {
  width: 100%;
  font-size: 30rpx;
  padding: 30rpx;
  border-top: 20rpx solid #EEEEEE;
}
.first-comment {
  
}
.comment-top {
  width: 100%;
  display: flex;
}
.user-avatar {
  flex: 1;
}
.user-avatar .img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.user-avatar .img image {
  width: 100rpx;
  height: 100rpx;
}
.user-info {
  flex: 7;
  padding-left: 40rpx;
  line-height: 50rpx;
}
.user-info-top {
  width: 100%;
  display: flex;
}
.user-name {
  flex: 2;
  text-align: left;
  color: #F05442;
}
.user-date {
  flex: 3;
  color: #848484;
}
.star {
  width: 100%;
}
.comment-text {
  width: 100%;
  line-height: 100rpx;
  border-bottom: 1px solid #ECECEC;
}
.comment-count {
  width: 100%;
  color: #F05442;
  line-height: 100rpx;
}
.more-hotel {
  width: 600rpx;
  margin: 0 auto 50rpx;
  background: #F05442;
  color: #fff;
  border-radius: 4px;
}
/*钟点房*/
.room-type-name .switch-more-hour {
  flex: 2;
}
.level-btn-hour {
  color: #fff;
  background-color: #F05442;
  border-radius: 4px;
  text-align: center;
  line-height: 50px;
  font-size: 26rpx;
  padding: 10rpx 20rpx;
}
.level-btn-hour.disabled-btn {
  background-color: #AAAAAA;
}

/* 更新版头部开始 */

.lc-head {
  width: 100%;
  background-color: #fff;
  /* border-top: 1px solid #F0F2F6; */
  /* height: 440rpx; */
  /* padding: 15rpx 30rpx; */
  /* box-sizing: border-box; */
  border-bottom: 22rpx solid #efefef;
}
.lc-headline {
  padding: 15rpx 30rpx;
  border-bottom: 1px solid #F0F2F6;
}
.lc-headimgs {
  height: 200rpx;
  /* background-color: yellow; */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  position: relative;
}
.lc-headimgs>image {
  width: 212rpx;
  height: 200rpx;
  border-radius: 10rpx;
}
.lc-collect {
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  top: 6rpx;
  right: 8rpx;
}
.lc-collect image {
  width: 64rpx;
  height: 64rpx;
}
.lc-imgnum {
  width: 88rpx;
  height: 44rpx;
  background-color: rgba(0, 0, 0, 0.4);
  position: absolute;
  border-radius: 22rpx;
  top: 150rpx;
  right: 10rpx;
  text-align: center;
  line-height: 40rpx;
  box-sizing: border-box;
  /* opacity: 0.3; */
}
.lc-imgnum image {
  width: 20rpx;
  height: 20rpx;
  vertical-align: middle;
  margin-right: 8rpx;
  opacity: 2;
}
.lc-imgnum text {
  color: #fff;
  font-size: 18rpx;
  opacity: 2;
  font-weight: bold;
}
.lc-headname {
  padding-top: 20rpx;
}
.lc-headname>text {
  width: 100%;
  height: 80rpx;
  /* line-height: 80rpx; */
  font-size: 32rpx;
  font-weight: bold;
  color: #000;
  margin-top: 20rpx;
}
.lc-headname .lchl-r1push {
  padding: 3rpx 9rpx;
  background-color: #F05442;
  color: #fff;
  font-size: 22rpx;
  border-radius: 8rpx;
  margin-right: 15rpx;
}
.lc-headaddress {
  /* height: 110rpx; */
  display: flex;
  flex-direction: row;
  position: relative;
}
.lc-adr {
  flex: 3;
  padding: 11rpx 38rpx;
  border-right: 1px solid #F0F2F6;
  /* box-sizing: border-box; */
}
.lc-adr>image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 21rpx;
}
.lc-adr>text {
  width: 480rpx;
  display: block;
  padding-left: 35rpx;
  overflow: hidden;
  /* white-space: nowrap; */
  text-overflow: ellipsis;
  color: #455055;
  font-size: 24rpx;
}
.lc-adr>text:first-of-type {
  margin-top: 10rpx;
}
.lc-adr>text:last-of-type {
  font-size: 20rpx;
  color: #A2A7AA;
  /* margin-top: 9rpx; */
}
.adcontainer{
  position:relative;
}
.nullinfo{
  width:100%;
  font-size:32rpx;
  text-align:center;
  padding-top:10rpx;
}
.aroundtit {
  height:86rpx;
  padding-left:40rpx;
  line-height:86rpx;
}
.aroundtit text {
  padding-left:20rpx;
  border-left:8rpx solid #f05442;
  font-size:32rpx;
  font-family:'PingFangSC';
  color:#455055;
  font-weight:bold;
}
.aroundlist{
  width:100%;
  display:flex;
  border-bottom:1px solid #eee;
  padding:26rpx 0;
}
.playaround>.aroundlist:last-child{
  border-bottom:0;
}
.leftimg {
  flex-direction:row;
  justify-content:space-between;
  position:relative;
  display:inline-block;
  margin-left:40rpx;
  margin-right:40rpx;
  float:left;
  background:#f0f2f6;
  width:256rpx;
  height:200rpx;
}
.nullimgs{
  position:absolute;
  top:50%;
  margin-top:-12%;
  left:0;
  z-index:0;
  color:#cccccc;
  font-size:46rpx;
  vertical-align:middle;
  width:100%;
  text-align:center;
}
.leftimg>image {
  width:100%;
  height:200rpx;
  border-radius:2rpx;
  position:absolute;
  left:0;
  right:0;
  z-index:1;
}
.rightcontlist{
  display:inline-block;
  float:left;
  line-height:46rpx;
}
.titname{
  font-size:30rpx;
  margin-bottom:12rpx;
  display:flex;
}
.aroundinfo>.a-score{
  color:#f5a623;
  font-size:28rpx;
  display:inline-block;
}
.aroundinfo>.a-call{
  color:#f5a623;
  font-size:28rpx;
  display:inline-block;
  position:relative;
  padding-left:66rpx;
}
.a-score text{
  border-right:1px solid #eee;
  padding-right:20rpx;
}
.a-call>image{
  width:32rpx;
  height:36rpx;
  position:absolute;
  left:24rpx;
  top:4rpx;
}
.adress-info{
  position:relative;
  padding-left:38rpx;
  color:#a2a7aa;
}
.adress-info>image{
  width:30rpx;
  height:30rpx;
  position:absolute;
  top:10rpx;
  left:0;
}
.a-monbox{
  margin-top:8rpx;
  color:#a2a7aa;
}
.a-money{
  color:#ed5548;
  font-size:40rpx;
}


/* 仿tabBar */
.copytabBar{
  position:fixed;
  bottom: 0;
  left:0;  
  z-index:10;
  width:100%;
  text-align:center;  
  border-top: 0.6px solid #ABACAF;
  background: #fff;
  display: flex;
  font-size:25rpx;  
  color: #909095;  
  padding-top:4px;
  margin-bottom:-1px;
}
.copytabBar>view{
  flex:1;    
}
.copytabBar>view>image{
  width:30px;
  height:30px;
  margin-bottom:-3px;
}