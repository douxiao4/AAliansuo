@import '../../template/stars-template.wxss';
.container {
  display: flex;
  flex-direction: column;
  width: 750rpx;
  overflow: hidden;
  /* background-color: #F0F2F6; */
  padding-bottom:110rpx;
}

.swiper {
  width: 750rpx;
  height: 400rpx;
}
.swiper image {
  width: 750rpx;
  height: 400rpx;
}
.hotel-title {
  width: 100%;
  font-size: 30rpx;
  line-height: 80rpx;
  color: #2a3342;
  padding: 0 50rpx;
}
.tags-price {
  width: 100%;
  padding: 0 50rpx;
  display: flex;
  padding-bottom: 40rpx;
  border-bottom: 1px solid #EBEDF0;
}
.tags-price .tags {
  flex: 2;
}
.tags-price .tags text {
  font-size: 18rpx;
  border: 1px solid #c6c7ca;
  padding: 4px;
  border-radius: 4px;
  margin-right: 4px;
}
.tags-price .price {
  flex: 1;
  color: #F05442;
}
.hotel-detail-phone {
  display: flex;
  line-height: 80rpx;
  font-size: 30rpx;
  padding: 20rpx 0;
}
.hotel-detail-phone text {
  flex: 1;
  text-align: center;
}
.hotel-detail-phone text:first-child {
  border-right: 1px solid #c6c7ca;
}
.maps {
  border-top: 20rpx solid #EEEEEE;
  /*height: 200rpx;*/
}
.maps map {
  width: 100%;
  /*height: 200rpx;*/
}
.address {
  width: 100%;
  color: #798394;
  padding: 0 50rpx;
  line-height: 100rpx;
  font-size: 26rpx;
  border-bottom: 20rpx solid #EEEEEE;
}

