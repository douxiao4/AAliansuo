/* pages/kangkang/praces.wxss */
page{
 background-color: #efeff4;
}
.img-tu{
  width: 100%;
  height: 320rpx;
  margin-bottom: 15rpx;
	display:flex;
}
.w70{
  width: 70px;
}
.w75{
  width: 75%;
}
.bule{
  color:rgb(67, 174, 231);
}
.login-input{
	padding: 17px 0;
	padding-left: 15px;
	position: relative;
	display: flex;
  background: #fff;
}
.login-input label{
  display: inline-block;
  font-size: 15px;
  box-sizing: border-box;
  height: 28px;
  line-height: 28px;
  flex: 1;
  padding-left: 20rpx;
}
.login-box-flex{
  justify-content: space-around;
  padding-left: 0px;
  display: flex;
}
.login-box-flex label{
  flex: 1;
}
.login-ipt{
  font-size: 13px;
  height: 28px;
}
.login-box-flex .login-ipt{
  flex: 2;
}
.login-box-flex .bule{
  flex: 1;
}
.login-btn{
	width: 90%!important;
	margin: 0 auto!important;
}
.login-submit{
  background-color: #F05442;
  width: 100%;
  margin: 0 auto;
  margin-top:20px;
  height: 40px;
  font-size: 30rpx;
  border-radius: 5px;
  border: 0px;
  color: #fff;
  display: block;
  text-align: center;
}
