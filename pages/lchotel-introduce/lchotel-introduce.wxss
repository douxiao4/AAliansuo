.container {
    width: 100%;
    /* height: 100%; */
    display: flex;
    flex-direction: column;
    background-color:#F0F2F6;
    position: relative;
}
/* 酒店名称开始 */
.lc-introHead {
    /* height: 150rpx; */
    padding: 36rpx 36rpx;
    background-color: #fff;
    border-top: 1px solid #F0F2F6;
    border-bottom: 1px solid #F0F2F6;
    border-top: 1px solid #F0F2F6;
    box-sizing: border-box;
}
.lc-introHead>text {
    color: #000;
    font-size: 32rpx;
    /* height: 90rpx; */
    /* line-height: 45rpx; */
    font-weight: bold;
    display: inline-block;
}
/* 酒店名称结束 */
/* 酒店电话开始 */
.lc-introTel {
    height: 128rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    padding: 36rpx 0 36rpx 36rpx;
    box-sizing: border-box;
    border-bottom: 1px solid #F0F2F6;
}
.lc-telLeft {
    flex: 3;
    display: flex;
    flex-direction: row;
    line-height: 60rpx;
    border-right: 1px solid #F0F2F6;
    color: #455055;
}
.lc-telLeft>image {
    width: 30rpx;
    height: 30rpx;
    margin-top: 15rpx;
    margin-right: 15rpx;
}
.lc-telRight {
    flex: 1;
    text-align: center;
}
.lc-telRight>image {
    width: 42rpx;
    height: 42rpx;
    margin-top: 12rpx;
}
/* 酒店电话结束 */
/* 酒店地址开始 */
.lc-introAdr {
    height: 128rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    padding: 26rpx 0 26rpx 36rpx;
    box-sizing: border-box;
    border-bottom: 1px solid #F0F2F6;
    margin-bottom: 20rpx;
}
.lc-adrLeft {
    flex: 3;
    display: flex;
    flex-direction: row;
    /* line-height: 60rpx; */
    border-right: 1px solid #F0F2F6;
    /* margin-top: 10rpx; */
}
.lc-adrLeft>image {
    width: 30rpx;
    height: 30rpx;
    margin-top: 15rpx;
    margin-right: 15rpx;
}
.lc-adrLeft>text {
    display: inline-block;
    margin-top: 15rpx;
    color: #455055;
}
.lc-adrRight {
    flex: 1;
    text-align: center;
}
.lc-adrRight>image {
    width: 42rpx;
    height: 42rpx;
    margin-top: 25rpx;
}
/* 酒店地址结束 */
/* 酒店设施开始 */
.lc-introFacility {
    background-color: #fff;
    margin-bottom: 20rpx;
}
.lc-facHead {
    height: 82rpx;
    font-size: 30rpx;
    color: #455055;
    line-height: 82rpx;
    padding-left: 36rpx;
    border-bottom: 1px solid #F0F2F6;
    font-weight: bold;
}
.lc-facInfo {
    padding: 36rpx 36rpx 0rpx 36rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}
.lc-facInfo>text {
    width: 25%;
    font-size: 24rpx;
    color: #455055;
    text-align: center;
    margin-bottom: 36rpx;
}
.lc-facInfoView {
    margin-bottom: 36rpx;
    color: #455055;
}
/* 酒店设施结束 */
/* 酒店介绍开始 */
.lc-introText {
    background-color: #fff;
    margin-bottom: 20rpx;
}
.lc-textHead {
    height: 82rpx;
    font-size: 30rpx;
    color: #455055;
    line-height: 82rpx;
    padding-left: 36rpx;
    border-bottom: 1px solid #F0F2F6;
    font-weight: bold;
}
.lc-text {
    padding: 36rpx;
}
.lc-text>text {
    color: #455055;
    line-height: 42rpx;
    font-size: 24rpx;
}
/* 酒店介绍结束 */
/* 酒店二维码开始 */
.lc-introText1 {
    background-color: #fff;
}
.lc-img {
    text-align: center;
}
.lc-img image {
    height: 400rpx;
    width: 345rpx;
    margin: 30rpx auto;
}

.back {
    position: absolute;
    top: 50%;
    width: 100%;
    height: 100%;
    background-color: #fff;
    transform: translateY(-50%); 
    overflow: hidden;
    text-align: center;
}
.back image {
    width: 90%;
    height: 750rpx;
    margin-top: 180rpx;
}
.goTo0 {
    opacity: 1;
    animation: changeOpacity0 0.75s linear 0s forwards !important;
}
.goTo1 {
    opacity: 0;
    animation: changeOpacity1 0.75s linear 0.75s forwards !important;
}
@keyframes changeOpacity1 {
    0% {opacity: 0}
    25% {opacity: 0.25}
    50% {opacity: 0.5}
    75% {opacity: 0.75}
    100% {opacity: 1}
}
@keyframes changeOpacity0 {
    0% {opacity: 1}
    25% {opacity: 0.75}
    50% {opacity: 0.5}
    75% {opacity: 0.25}
    100% {opacity: 0}
}

/* 酒店二维码结束 */
