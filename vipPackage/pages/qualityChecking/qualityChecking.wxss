.page {
  width: 100%;
  height: 1473rpx;
  display: flex;
  flex-flow: column;
  background: #f1f1f1;
}
.page .box {
  width: 100%;
  height: 100%;
  background: url(https://ucpic.aaroom.cc/2019-01-31_5c5281e2c8140.jpg) no-repeat left top;
  background-size: 100% 100%;
}
.page .box .titleBig {
  margin-top: 403rpx;
  margin-bottom: 15rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 50rpx;
  color: #00cbc0;
}
.page .box .txt {
  margin-top: 10rpx;
  height: 33rpx;
  line-height: 33rpx;
  font-size: 24rpx;
  color: #666;
  text-align: center;
}
.page .box .txtBig {
  margin-top: 10rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #666;
  text-align: center;
}
.page .box .txtBig .big {
  line-height: 40rpx;
  font-size: 28rpx;
  color: #00cbc0;
}
.page .box .btn {
  display: block;
  margin: 9rpx auto 6rpx;
  width: 426rpx;
}
.page .box .btn:active {
  opacity: 0.6;
}
.page .box .imageRight {
  margin-top: 49rpx;
  width: 100%;
  text-align: right;
}
.page .box .imageRight image {
  width: 505rpx;
  display: inline-block;
}
.page .box .bottomBox {
  width: 100%;
  padding-left: 67rpx;
  display: flex;
  font-display: row;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}
.page .box .bottomBox .left {
  width: 219rpx;
}
.page .box .bottomBox .left .title {
  padding-bottom: 10rpx;
  line-height: 37rpx;
  font-weight: 600;
  font-size: 26rpx;
  color: #333333;
  border-bottom: 1px solid #979797;
}
.page .box .bottomBox .left .list {
  width: 100%;
}
.page .box .bottomBox .left .list .itemBox {
  margin-top: 22rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.page .box .bottomBox .left .list .itemBox .item {
  line-height: 33rpx;
  height: 33rpx;
  font-size: 24rpx;
  color: #049A91;
}
.page .box .bottomBox .left .list .jLeft.itemBox {
  justify-content: flex-start;
}
.page .box .bottomBox .left .list .jLeft.itemBox .item {
  margin-right: 38rpx;
}
.page .box .bottomBox .right image {
  display: block;
  width: 426rpx;
}
